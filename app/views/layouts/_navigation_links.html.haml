%li= link_to 'About French House', about_path
%li= link_to 'Join French House', join_path
%li= link_to 'Rules', rules_path
%li= link_to 'Recipes', recipes_path
%li= link_to 'Ingredients', ingredients_path
%li= link_to 'Contact Us', contact_path
- if user_signed_in?
  %li= link_to 'Labor', labors_path
  %li= link_to 'Edit account', edit_user_registration_path
  %li= link_to 'Sign out', destroy_user_session_path, method: 'delete'
- else
  %li= link_to 'Sign in', new_user_session_path
  %li= link_to 'Sign up', new_user_registration_path
- if user_signed_in?
  - if current_user.admin?
    %li= link_to 'Admin', '/admin'
    %li= link_to 'Users', users_path
